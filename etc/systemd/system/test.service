[Unit]
Description=Seconionis Tor Traffic Routing
After=network-online.target tor.service
Wants=network-online.target tor.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStartPre=/usr/bin/test -x /usr/bin/seconionis
ExecStart=/usr/bin/seconionis start
ExecReload=/usr/bin/seconionis restart
ExecStop=/usr/bin/seconionis stop
TimeoutStartSec=120
TimeoutStopSec=60

[Install]
WantedBy=multi-user.target
